<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="feedDAO">
	
	<select id="getFeedList" resultType="feed">
		SELECT F_IDX, REGDATE, CONTENT, U_ID, F_PIC
		  FROM FEED
	</select>
	
	
  	
